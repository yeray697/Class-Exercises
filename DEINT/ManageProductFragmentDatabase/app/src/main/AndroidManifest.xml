<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yrj.manageproductrecycler">

    <application
        android:name=".Login_Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login_Activity"/>
        <activity android:name=".GeneralSetting_Activity" />
        <activity android:name=".AccountSetting_Activity" />
        <activity android:name=".SignUp_Activity" />
    </application>

</manifest>